<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/gotoweb"
        android:layout_width="359dp"
        android:layout_height="457dp"
        android:background="@drawable/mainpage_box"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="200dp"
        android:layout_height="194dp"
        android:src="@drawable/hand"
        app:layout_constraintBottom_toBottomOf="@+id/gotoweb"
        app:layout_constraintEnd_toEndOf="@+id/gotoweb"
        app:layout_constraintStart_toStartOf="@+id/gotoweb"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="180dp"
        android:layout_height="57dp"
        android:text="Go to Web!"
        android:textAppearance="@style/title"
        android:textColor="#FFB01D"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/gotoweb"
        app:layout_constraintEnd_toEndOf="@+id/gotoweb"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/gotoweb"
        app:layout_constraintTop_toTopOf="@+id/gotoweb"
        app:layout_constraintVertical_bias="0.097" />

    <Button
        android:id="@+id/nextpage"
        android:layout_width="117dp"
        android:layout_height="80dp"
        android:text="임시버튼"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.911"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gotoweb"
        app:layout_constraintVertical_bias="0.719" />


</androidx.constraintlayout.widget.ConstraintLayout>